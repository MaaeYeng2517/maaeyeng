{
    "server": {
        "servername": "MaaeyengServer",
        "port": 8080,
        "host": "localhost",
        "ssl": {
            "enabled": false,
            "keyPath": "/path/to/key.pem",
        "certPath": "/path/to/cert.pem"
    },
    "timeout": 120000,
    "maxConnections": 1000
    },
    "logging": {
        "level": "info",
        "filePath": "/var/log/maaeyeng/server.log",
        "maxSizeMB": 50,
        "maxFiles": 5
    }
}